<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="VARCHAR" hibernate-type="java.lang.String">
		</sql-type>
		<sql-type jdbc-type="CHAR" hibernate-type="java.lang.String">
		</sql-type>
		<sql-type jdbc-type="TIMESTAMP" hibernate-type="java.util.Date">
		</sql-type>
		<sql-type jdbc-type="DECIMAL" hibernate-type="java.lang.Long" scale="0">
		</sql-type>
		<sql-type jdbc-type="DECIMAL" hibernate-type="java.math.BigDecimal">
		</sql-type>
	</type-mapping>
	<table-filter match-name="pro_lang" match-schema="testhibernate"></table-filter>    
</hibernate-reverse-engineering>